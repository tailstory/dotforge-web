{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "dotforge",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2026-01-20",
  "compatibility_flags": ["nodejs_compat"],
  "version_metadata": {
    "binding": "CF_VERSION_METADATA"
  },
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  },
  "placement": {
    "mode": "smart"
  },
  "upload_source_maps": true,
  "preview_urls": true
}
